#!/bin/bash
# WhisperDictate launcher - this file is regenerated by install.sh with your paths
# If you see this message, run: cd ~/whisper-dictate && bash install.sh

SCRIPT_DIR="$(cd "$(dirname "$0")/../../.." && pwd)"

export PATH="/opt/homebrew/bin:/usr/local/bin:$PATH"

cd "$SCRIPT_DIR"
exec "$HOME/whisper-official/bin/python3" "$SCRIPT_DIR/dictate.py"
